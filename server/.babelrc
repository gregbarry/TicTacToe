{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": "current"
            }
        }]
    ],
    "plugins": [
        ["module-resolver", {
            "alias": {
                "-": "./app"
            }
        }],
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-async-to-generator"
    ]
}
